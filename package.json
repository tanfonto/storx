{
  "name": "@tanfonto/storx",
  "version": "0.1.0",
  "description": "Dead simple state management",
  "keywords": [
    "state management",
    "store",
    "redux",
    "reactive",
    "observable",
    "frp",
    "rxjs",
    "tiny",
    "simple",
    "typescript"
  ],
  "main": "dist/index.ts",
  "scripts": {
    "lint": "tslint -p tsconfig.json",
    "test:dev": "tsc && nyc ava --tap | tap-diff",
    "test": "tsc && nyc ava && nyc report --reporter=text-lcov | coveralls",
    "watch": "ava --verbose --watch",
    "build": "tsc -p tsconfig.app.json",
    "prepublish": "npm run build  "
  },
  "repository": {
    "url": "https://github.com/tanfonto/storx"
  },
  "author": "tanfonto",
  "license": "MIT",
  "dependencies": {
    "ava": "^0.25.0",
    "ava-ts": "^0.25.2",
    "coveralls": "^3.0.2",
    "nyc": "^13.1.0",
    "rxjs": "^6.3.3",
    "tap-diff": "^0.1.1",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.1.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
